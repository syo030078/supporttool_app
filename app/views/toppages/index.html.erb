
<% if logged_in? %>
  <p>ログイン完了ユーザ: <%= current_user.name %></p>
  <% @item_lists.each do |item_list| %>
    <p><%= link_to item_list.id, item_list_path(item_list) %><%= item_list.list_name %></p>
<% end %>

<% else %>
<div>
  <div>
    <h1>Welcome to the Supporttool</h1>
    <%= link_to 'Sign up now!', signup_path %>
  </div>
</div>
<% end %>